<?xml version="1.0" encoding="utf-8"?>
<configdata>
  <routing>
   <routes>
        <helper>
            <type>Zend_Controller_Router_Route</type>
			<route>:module/:controller/:action/*</route>
            <defaults>
              <module>helper</module>
              <controller>index</controller>
              <action>index</action>
            </defaults>
        </helper>
    </routes>
  </routing>
</configdata>
